{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "semi": [1, "always"],
    "import/newline-after-import": 0,
    "react/jsx-filename-extension": 0,
    "comma-dangle": ["error", "never"],
    "react/jsx-curly-spacing": [2, { "when": "always", "spacing": { "objectLiterals": "never" }}]
  }
}
